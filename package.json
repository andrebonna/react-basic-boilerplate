{
  "name": "react-basic-boilerplate",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack-dev": "webpack -w",
    "webpack-prod": "NODE_ENV=production webpack",
    "express-server": "node ./Server/app",
    "lint": "eslint --quiet View/src/** Server/** *.js",
    "dev": "concurrently --kill-others \"npm run webpack-dev\" \"npm run express-server\"",
    "start": "npm run lint && npm run webpack-prod && npm run express-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.8.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^17.0.0",
    "jest-cli": "^17.0.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.1.1",
    "webpack": "^1.13.2"
  }
}
